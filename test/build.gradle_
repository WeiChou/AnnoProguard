 */

repositories {
    jcenter()
}

apply plugin: 'java'

sourceSets {
    main {
        java {
            srcDirs 'src'
        }
    }
    test {
    }
}

dependencies {
    compile project.parent
    compile 'com.google.code.gson:gson:2.8.0'
}

javadoc {
    options {
        encoding 'UTF-8'
        charSet 'UTF-8'
    }
}
